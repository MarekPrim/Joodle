<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox minWidth="800.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ControllerTaches">
   <children>
      <fx:include source="barre_superieure.fxml" />
      <HBox VBox.vgrow="ALWAYS">
         <children>
            <fx:include source="menu.fxml" />
            <VBox prefHeight="338.0" prefWidth="580.0">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <HBox prefHeight="100.0" prefWidth="652.0">
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="A faire">
                                 <HBox.margin>
                                    <Insets left="5.0" right="20.0" top="35.0" />
                                 </HBox.margin>
                                 <font>
                                    <Font size="22.0" />
                                 </font>
                              </Text>
                              <ListView fx:id="a_faire" onMouseClicked="#finir" prefHeight="100.0" prefWidth="510.0">
                                 <HBox.margin>
                                    <Insets left="5.0" top="5.0" />
                                 </HBox.margin>
                              </ListView>
                           </children>
                        </HBox>
                        <VBox prefHeight="97.0" prefWidth="140.0">
                           <children>
                              <Button fx:id="up" mnemonicParsing="false" onMouseClicked="#reorder" text="△">
                                 <VBox.margin>
                                    <Insets top="15.0" />
                                 </VBox.margin></Button>
                              <Button fx:id="down" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#reorder" text="▽">
                                 <VBox.margin>
                                    <Insets top="22.0" />
                                 </VBox.margin></Button>
                           </children>
                           <HBox.margin>
                              <Insets left="10.0" />
                           </HBox.margin>
                        </VBox>
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="ajouter_taches" mnemonicParsing="false" onMouseClicked="#ajouter_taches" text="Ajouter" />
                        <TextField fx:id="tache" onKeyPressed="#entree">
                           <HBox.margin>
                              <Insets left="15.0" />
                           </HBox.margin></TextField>
                     </children>
                     <padding>
                        <Insets left="45.0" top="35.0" />
                     </padding>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Fini">
                           <HBox.margin>
                              <Insets left="5.0" right="20.0" top="35.0" />
                           </HBox.margin>
                           <font>
                              <Font size="22.0" />
                           </font>
                        </Text>
                        <ListView fx:id="fini" onMouseClicked="#reprendre" prefHeight="100.0" prefWidth="452.0">
                           <HBox.margin>
                              <Insets left="35.0" />
                           </HBox.margin>
                        </ListView>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
