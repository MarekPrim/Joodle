<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox minWidth="800.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ControllerTaches">
   <children>
      <fx:include source="barre_superieure.fxml" />
      <HBox VBox.vgrow="ALWAYS">
         <children>
            <fx:include source="menu.fxml" />
            <VBox prefHeight="338.0" prefWidth="580.0">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <ListView fx:id="a_faire" onMouseClicked="#finir" prefHeight="97.0" prefWidth="619.0" />
                        <VBox prefHeight="97.0" prefWidth="140.0">
                           <children>
                              <Button fx:id="up" mnemonicParsing="false" onMouseClicked="#reorder" text="△" />
                              <Button fx:id="down" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#reorder" text="▽" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="ajouter_taches" mnemonicParsing="false" onMouseClicked="#ajouter_taches" text="Ajouter" />
                        <TextField fx:id="tache" onKeyPressed="#entree" />
                     </children>
                  </HBox>
                  <ListView fx:id="fini" onMouseClicked="#reprendre" prefHeight="190.0" prefWidth="414.0" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
